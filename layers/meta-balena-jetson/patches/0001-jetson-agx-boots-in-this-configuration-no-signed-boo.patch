+signfile() {
+    local destdir="${WORKDIR}/tegraflash"
+    local lnxfile="${LNXFILE}"
+    local f
+    PATH="${STAGING_BINDIR_NATIVE}/tegra186-flash:${PATH}"
+    export cbootfilename=cboot_t194.bin
